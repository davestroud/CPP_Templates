#include <iostream>
using namespace std;

int main()
{
  int number;
  
  cout << "Enter a number in the range of 1 to 10" << endl;
  cin >> number;

  while (number < 1 || > number 10)
    {
      cout << "Error: Enter a number between 1 and 10" << endl;
      cin >> number;
    }
  
  return 0;
}
